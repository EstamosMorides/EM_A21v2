    <!-- RESTORED -->
    <append xpath="/blocks">
        <block name="restoredOvens_master">
            <property name="CreativeMode" value="None"/>
            <!-- Class -->
            <property name="Class" value="Campfire"/>
            <property name="Group" value="Basics,Food/Cooking,Building,advBuilding"/>
            <property class="Workstation">
                <property name="CraftingAreaRecipes" value="campfire"/>
                <property name="Modules" value="tools,output,fuel,input"/>
                <property name="Collide" value="melee,bullet,arrow,rocket"/>
            </property>

            <property name="CanPickup" value="true"/>
            <property name="TakeDelay" value="5"/>
            <property name="ActivationDistance" value="3"/>
            <!-- UI_Info -->
            <property name="ItemTypeIcon" value="vise"/>
            <property name="DescriptionKey" value="campfireDesc"/>
            <property name="PickupJournalEntry" value="campfireTip"/>
            <property name="WorkstationIcon" value="ui_game_symbol_campfire"/>
            <property name="WorkstationJournalTip" value="heatMapTip"/>
            <property name="CraftIcon" value="ui_game_symbol_spatula"/>
            <property name="CraftActionName" value="lblContextActionCook"/>
            <!-- Visual -->
            <property name="Material" value="Mmetal"/>
            <property name="Shape" value="ModelEntity"/>
            <property name="Model" value="@:Entities/LootContainers/wallOvenFullPrefab.prefab"/>
            <property name="WaterFlow" value="permitted"/>
            <!-- Placement -->
            <property name="Place" value="TowardsPlacerInverted"/>
            <property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
            <!-- Interaction -->
            <property name="HeatMapStrength" value="5"/>
            <property name="HeatMapTime" value="5000"/>
            <property name="HeatMapFrequency" value="1000"/>
            <property name="ActiveRadiusEffects" value="buffCampfireAOE(3)"/>
            <property name="CraftSound" value="campfire_cook_click"/>
            <property name="CraftCompleteSound" value="campfire_complete_item"/>
            <!-- Sorting -->
            <property name="FilterTags" value="MC_building,SC_electrical,A21_NEW"/>
        </block>

        <!-- region WALL_OVENS -->
        <block name="wallOvenWhite_restored">
            <property name="Extends" value="restoredOvens_master"/>
            <property name="CreativeMode" value="Player"/>
            <!-- UI_Info -->
            <property name="CustomIcon" value="cntwallOvenFullWhite"/>
            <property name="CustomIconTint" value="A0A0A0"/>
            <property name="TintColor" value="A0A0A0"/>
            <!-- Visual -->
            <property name="Model" value="@:Entities/LootContainers/wallOvenFullPrefab.prefab"/>
            <property name="ModelOffset" value="0,0.5,-1"/>
            <property name="ParticleName" value="chemistryStation"/>
            <property name="ParticleOffset" value="-0.5,0.5,-0.5"/>

            <property class="RepairItems">
                <property name="resourceMechanicalParts" value="1"/>
                <property name="resourceMetalPipe" value="2"/>
            </property>
        </block>

        <block name="wallOvenGrey_restored">
            <property name="Extends" value="restoredOvens_master"/>
            <property name="CreativeMode" value="Player"/>

            <!-- UI_Info -->
            <property name="CustomIcon" value="cntwallOvenFullWhite"/>
            <property name="CustomIconTint" value="646464"/>
            <!-- Visual -->
            <property name="Model" value="@:Entities/LootContainers/wallOvenFullPrefab.prefab"/>
            <property name="ModelOffset" value="0,0.5,-1"/>
            <property name="TintColor" value="646464"/>

            <property name="ParticleName" value="chemistryStation"/>
            <property name="ParticleOffset" value="-0.5,0.5,-0.5"/>

<!--            <dropextendsoff/>-->
            <drop event="Destroy" name="wallOvenGrey_restored" count="1"/>

            <property class="RepairItems">
                <property name="resourceMechanicalParts" value="1"/>
                <property name="resourceMetalPipe" value="2"/>
            </property>
